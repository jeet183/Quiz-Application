<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeetmishra.androidquizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NextScreen" />
        <activity android:name=".ThirdScreen" />
        <activity android:name=".SignUp" />
        <activity android:name=".Home" />
        <activity android:name=".Questions" />
        <activity android:name=".Start" />
        <activity android:name=".Playing" />
        <activity android:name=".Done" />
        <activity android:name=".Online" />
        <activity android:name=".HeadOn" />
        <activity android:name=".OnlinePlaying" />
        <activity android:name=".ChangeTime" />
        <activity android:name=".ScoreDetail"></activity>
    </application>

</manifest>